if ("<%= @unfiltered %>" == 'true') {
  document.querySelector('.organisation-main-container').classList.remove('hidden')
  document.querySelector('.organisation-main-container-filtered').classList.add('hidden')
} else {
  document.querySelector('.organisation-main-container').classList.add('hidden')
  document.querySelector('.organisation-main-container-filtered').classList.remove('hidden')
  document.querySelector('.organisation-main-container-filtered').innerHTML = '<%= j render("pages/organisation_ajax", users: @users, unfiltered: @unfiltered) %>';
}
$(document).ready(function () {
  $(".organisation-page").floatingScroll();
});
document.querySelector('body').classList.remove('wait');

