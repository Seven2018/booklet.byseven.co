document.querySelector('#lines').innerHTML = '<%= j render("interview_forms/index/index_interview_forms_list", templates: @templates) %>';

hideOverflowingTags()

document.querySelector('body').classList.remove('wait')
document.getElementById('bkt-blockDiv').classList.add('d-none')

document.getElementById('stimulus-change-page-controller').changePage._setPaginationLinks()

el = document.querySelectorAll('.setup-search-arg')

if (el) {
  el.forEach(a => {
    const text = localStorage.getItem('bookletInterviewFormsSearches')

    if (text) {
      a.href += `?back_search_text=${text}`
    }
  })
  localStorage.setItem('bookletInterviewFormsSearches', '')
}