document.querySelector('.organisation-main').innerHTML = '<%= j render("pages/organisation_users_list", users: @users, unfiltered: @unfiltered) %>';
document.querySelector('.organisation-filter-dropright').innerHTML = '<%= j render("pages/organisation_filter_dropright") %>';
document.querySelector('.organisation-manage-users-dropright').innerHTML = '<%= j render("pages/organisation_manage_users_dropright", users: @users, tag_categories: @tag_categories) %>';
$(document).ready(function () {
  $(".organisation-page").floatingScroll();
});
document.querySelector('body').classList.remove('wait');
tag_category_names = document.querySelectorAll('#tag_category_name')
tag_category_names.forEach((element) => {
  element.addEventListener('keypress', function(e) {
    if (event.which == '13') {
      spinningLoader();
    }
  })
})
